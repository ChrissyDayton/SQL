SELECT * FROM pets WHERE cost_in_dollars > 100
GROUP BY cost_in_dollars;


--Created a table listing cost of animals at a pet store
CREATE TABLE pets(id INTEGER PRIMARY KEY, animal TEXT, cost_in_dollars INTEGER);


--Select from groups animals that cost more than $100
SELECT * FROM pets WHERE cost_in_dollars > 100
GROUP BY cost_in_dollars;
